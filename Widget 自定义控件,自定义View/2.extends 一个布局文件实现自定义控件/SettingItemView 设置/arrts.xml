<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--声明本xml是SettingItemView的属性-->
    <declare-styleable name="SettingItemView">

        <!--自定义标题属性,注意string是小写★★★★★★★★★★★★★★★★★★-->
        <attr name="Setting_Title" format="string"/>

        <!--背景属性-->
        <attr name="Setting_Background">
            <enum name="first" value="0"/>
            <enum name="middle" value="1"/>
            <enum name="last" value="2"/>
        </attr>

        <!--是否显示开关图片-->
        <attr name="ToogleVisiable" format="boolean"/>

    </declare-styleable>
</resources>