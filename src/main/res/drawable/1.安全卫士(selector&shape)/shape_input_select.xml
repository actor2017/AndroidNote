<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
       android:shape="rectangle">

    <!--圆角-->
    <corners
        android:radius="10dp"/>

    <!--内边距-->
    <padding
        android:left="10dp"
        android:right="10dp"
        android:top="10dp"
        android:bottom="10dp"/>

    <!--内部填充,纯色-->
    <solid
        android:color="#ffff9d77"/>

    <!--描边-->
    <stroke
        android:width="2dp"
        android:dashWidth="2dp"
        android:dashGap="3dp"
        android:color="#cccccc"/>
</shape>